        "import numpy as np"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def llaves_colores(r,n):\n",
        "    sol = (np.math.factorial(r+n-1))/(np.math.factorial(r) * np.math.factorial(n-1))-3\n",
        "    return sol"
      ],
      "metadata": {
