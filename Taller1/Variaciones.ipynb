

def factorial(num): 
    if num == 0 or num == 1:
        return 1
        
    else: 
        factor = 1
        while(num > 1): 
            factor *= num 
            num -= 1
        return factor
    
def variaciones(n,r):
    resta = n-r
    return (factorial(n))/factorial(resta)


print (variaciones(6,3))
    